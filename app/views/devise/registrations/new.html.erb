    <div class="up">
     <h3>新規会員登録</h3>
   </div>
   <div class="userheadline">
     <div class="container"> 
       <div class="row"> 
         <div class="col-xs-3">
            <div class="sinupheadline">
              <div class="sinuphead">
                <h4>名前</h4>
              </div>
              <div class="sinuphead">
                <h4>フリガナ</h4>
              </div>
              <div class="sinuphead">
                <h4>メールアドレス</h4>
              </div>
              <div class="sinuphead">
                <h4>郵便番号</h4>
              </div>
              <div class="sinuphead">
                <h4>住所</h4>
              </div>
              <div class="sinuphead">
                <h4>電話番号</h4>
              </div>
              <div class="sinuphead">
                <h4>パスワード(6文字)</h4>
              </div>
              <div class="sinuphead">
                <h4>パスワード(確認用)</h4>
              </div>
            </div>
              <div class="pu">
               <h3>既に登録済みの方</h3>
                <div class=pu1>
                  <P><%= link_to "こちら", new_end_user_session_path %> からログインしてください。</P>
                </div>
             </div>
         </div>
         <div class="col-xs-9">
          <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= render "devise/shared/error_messages", resource: resource %>
            <div class="tableline">
              <table class="table">
                <div class="field">
                  <%= f.label :姓 %>
                  <%= f.text_field :family_name, autofocus: true, autocomplete: "family_name" %>
                </div>
                <div class="field">
                  <%= f.label :名 %>
                  <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
                </div>
              </table>
              <table class="table">
                <div class="field">
                  <%= f.label :セイ %>
                  <%= f.text_field :family_name_kana, autofocus: true, autocomplete: "family_name_kana" %>
                </div>
                <div class="field">
                  <%= f.label :メイ %>
                  <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "first_name_kana" %>
               </div>
             </table>
           </div>
           <div class="tableline1">
             <table class="table">
               <div class="field">
                 <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
               </div>
             </table>
             <table class="table">
               <div class="field">
                 <%= f.text_field :post_code, autofocus: true, autocomplete: "post_code" %>
               </div>
             </table>
          </div>
          <div class="tableline2">
            <table class="table">
              <div class="field">
                <%= f.text_field :address, autofocus: true, autocomplete: "address", :size => "45" %>
              </div>
            </table>
          </div>
          <div class="tableline1">
            <table class="table">
              <div class="field">
                <%= f.text_field :phone_number, autofocus: true, autocomplete: "phone_number" %>
              </div>
            </table>
            <table class="table">
              <div class="field">
                <% if @minimum_password_length %>
                <% end %>
                <%= f.password_field :password, autocomplete: "new-password" %>
              </div>
            </table>
            <table class="table">
              <div class="field">
                <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
              </div>
            </table>
           </div>
         <div class="botton">
            <table class="table">
           <div class="actions">
             <%= f.submit "新規登録", class:"btn btn-success btn-lg" %>
           </div>
         </div>
         </table>
        </div>
        <% end %>