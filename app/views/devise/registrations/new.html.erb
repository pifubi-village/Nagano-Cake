    <div class="up">
      <h3><span class="gray">　新規会員登録　</span></h3>
   </div>
   <div class="userheadline">
     <div class="container"> 
       <div class="row"> 
         <div class="col-xs-4">
            <div class="sinupheadline">
              <div class="sinuphead">
                <h5>名前</h5>
              </div>
              <div class="sinuphead">
                <h5>フリガナ</h5>
              </div>
              <div class="sinuphead">
                <h5>メールアドレス</h5>
              </div>
              <div class="sinuphead">
                <h5>郵便番号</h5>
              </div>
              <div class="sinuphead">
                <h5>住所</h5>
              </div>
              <div class="sinuphead">
                <h5>電話番号(ハイフンなし)</h5>
              </div>
              <div class="sinuphead">
                <h5>パスワード(6文字以上)</h5>
              </div>
              <div class="sinuphead">
                <h5>パスワード(確認用)</h5>
              </div>
            </div>
              <div class="pu">
               <h3><span class="gray">　既に登録済みの方　</span></h3>
                <div class=pu1>
                  <P><%= link_to "こちら", new_end_user_session_path %> からログインしてください。</P>
                </div>
             </div>
         </div>
         <div class="col-xs-8">
          <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= render "devise/shared/error_messages", resource: resource %>
            <div class="tableline">
              <table class="table">
                <div class="field">
                  <tr> (姓) </tr>
                  <%= f.text_field :family_name, autofocus: true, autocomplete: "family_name" %>
                </div>
                <div class="field">
                   <tr> (名) </tr>
                  <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
                </div>
              </table>
              <table class="table">
                <div class="field">
                   <tr> (セイ) </tr>
                  <%= f.text_field :family_name_kana, autofocus: true, autocomplete: "family_name_kana" %>
                </div>
                <div class="field">
                     <tr> (メイ) </tr>
                  <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "first_name_kana" %>
               </div>
             </div>
             </table>
           </div>
           <div class="tableline1">
             <table class="table">
               <div class="field">
                 <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
               </div>
             </table>
             <table class="table">
               <div class="field">
                 <%= f.text_field :post_code, autofocus: true, autocomplete: "post_code" %>
               </div>
             </table>
          </div>
          <div class="tableline2">
            <table class="table">
              <div class="field">
                <%= f.text_field :address, autofocus: true, autocomplete: "address", :size => "45" %>
              </div>
            </table>
          </div>
          <div class="tableline1">
            <table class="table">
              <div class="field">
                <%= f.text_field :phone_number, autofocus: true, autocomplete: "phone_number" %>
              </div>
            </table>
            <table class="table">
              <div class="field">
                <% if @minimum_password_length %>
                <% end %>
                <%= f.password_field :password, autocomplete: "new-password" %>
              </div>
            </table>
            <table class="table">
              <div class="field">
                <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
              </div>
            </table>
           </div>
         <div class="botton">
            <table class="table">
           <div class="actions">
             <%= f.submit "新規登録", class:"btn btn-success btn-lg" %>
           </div>
         </div>
         </table>
        </div>
        <% end %>