商品一覧


<table>
	<thead>
		<tr>
			<th>商品ID</th>
			<th>商品名</th>
			<th>ジャンル</th>
		</tr>
	</thead>
	<tbody>
		<% @products.each do |product| %>
		<tr>
			<td><%= product.id %></td>
			<td><%= product.name %></td>
			<td><%= product.genre.name %></td>
		</tr>
		<% end %>
	</tbody>
</table>



