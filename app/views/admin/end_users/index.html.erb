<div class="container">
	<div class="row">

		<div class="col-md-3">
		</div>

		<div class="col-md-6">
			<h2>会員一覧</h2>

			<br>

			<table class="table">
				<thead>
					<tr>
						<th>会員ID</th>
						<th>氏名</th>
						<th>メールアドレス</th>
						<th>ステータス</th>
					</tr>
				</thead>

				<tbody>
					<% @end_users.each do |user| %>
					<tr>
						<td><%= user.id %></td>
						<td><%= user.family_name %><%= user.first_name %></td>
						<td><%= user.email %></td>
						<td>有効とか</td>
					</tr>
					<% end %>
				</tbody>
			</table>

			<div class='p-eu__paginate'>
				<%= paginate @end_users %>
			</div>
		</div>

	</div>
</div>
